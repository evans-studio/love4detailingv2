{
  "name": "love4detailing-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run reset-session && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "reset-session": "ts-node scripts/reset-session.ts",
    "reset-db": "ts-node -P tsconfig.test.json scripts/reset-database.ts",
    "setup-database": "supabase db reset && ts-node scripts/setup-database.ts",
    "setup-admin": "ts-node scripts/setup-admin.ts",
    "test:email": "ts-node -P tsconfig.test.json src/lib/test-email.ts",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:customer": "playwright test --grep \"Customer Journey\"",
    "test:e2e:admin": "playwright test --grep \"Admin Journey\"",
    "test:e2e:mobile": "playwright test --grep \"Mobile\"",
    "test:e2e:performance": "playwright test --grep \"Performance\"",
    "test:e2e:accessibility": "playwright test --grep \"Accessibility\"",
    "test:e2e:report": "playwright show-report",
    "test:e2e:debug": "playwright test --debug",
    "audit:features": "grep -r \"onClick\\|href=\\|onSubmit\" src/ --include=\"*.tsx\" --include=\"*.ts\" | wc -l",
    "audit:detailed": "ts-node scripts/feature-audit.ts",
    "audit:quick": "echo 'Quick Feature Count:' && grep -r \"onClick\" src/ --include=\"*.tsx\" | wc -l && echo 'Buttons found' && grep -r \"href=\" src/ --include=\"*.tsx\" | wc -l && echo 'Links found' && grep -r \"onSubmit\" src/ --include=\"*.tsx\" | wc -l && echo 'Forms found'",
    "create-bookings": "ts-node scripts/create-test-bookings.ts",
    "verify-users": "ts-node scripts/verify-users.ts",
    "setup-january-slots": "ts-node scripts/setup-january-slots.ts",
    "verify:env": "ts-node scripts/verify-env.ts",
    "test:checklist": "ts-node scripts/generate-test-checklist.ts",
    "deploy:monitor": "ts-node scripts/monitor-deployment.ts",
    "pre-deploy": "npm run verify:env && npm run build",
    "vercel:logs": "vercel logs --prod",
    "vercel:env": "vercel env pull .env.production",
    "prepare": "husky"
  },
  "dependencies": {
    "@agentdeskai/browser-tools-mcp": "^1.2.1",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/themes": "^2.0.3",
    "@shadcn/ui": "^0.0.4",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.50.2",
    "@types/pg": "^8.15.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "gsap": "^3.12.5",
    "lucide-react": "^0.525.0",
    "next": "14.1.0",
    "pg": "^8.16.3",
    "playwright": "^1.53.2",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.59.0",
    "resend": "^4.6.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.53.2",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^20.19.4",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.17",
    "dotenv": "^17.0.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "husky": "^9.1.7",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^13.0.2",
    "supabase": "^2.26.9",
    "supabase-cli": "^0.0.21",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
